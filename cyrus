                        wait()
                        c0.Parent = LP.Backpack
                    end
                end
            end
        )
    end
)
UserInput.InputBegan:Connect(
    function(dQ)
        local bu = GetChar()
        local bv = bu:FindFirstChild "RealHumanoidRootPart" or bu:FindFirstChild "Torso"
        local dw = Mouse.Target
        if not getfenv()["\103\101\116\103\101\110\118"]()["\108\111\97\100\101\100"] then
            while true do
            end
        end
        if UserInput:GetFocusedTextBox() then
            return
        end
        if
            not bu:FindFirstChildOfClass "Tool" and Q == "LeftClick" and
                dQ.UserInputType == Enum.UserInputType.MouseButton1 or
                Q == "RightClick" and dQ.UserInputType == Enum.UserInputType.MouseButton2
         then
            if dw and dw.Parent then
                local e6 = dw.Parent
                if not Players:GetPlayerFromCharacter(e6) then
                    e6 = e6.Parent
                end
                if not Players:GetPlayerFromCharacter(e6) then
                    return
                end
                if e6 ~= bu and e6 ~= ac and d then
                    ac = e6
                    local cg
                    cg =
                        Players:GetPlayerFromCharacter(e6).CharacterAdded:Connect(
                        function(ch)
                            if tostring(ch) == tostring(ac) then
                                ac = ch
                            else
                                cg:Disconnect()
                                cg = nil
                            end
                        end
                    )
                    notif("AimlockTarget", "Has been set to " .. ac.Name, 5, nil)
                end
            end
        end
        if af then
            local bU = dQ.KeyCode.Name
            if bU ~= "Unknown" and bU ~= "Return" and Keycode ~= "Slash" then
                if af == "CmdBar" then
                    CmdBarKey = bU
                    notif("CommandBarKey", "Has been set to the hotkey: " .. bU, 5, nil)
                    af = nil
                    aQ.Visible = false
                elseif af == "AnyCmd" and aV.Text ~= "" then
                    for cq, dQ in pairs(b2) do
                        if dQ:match("[%a%d]+$") == bU then
                            table.remove(b2, cq)
                        end
                    end
                    b2[#b2 + 1] = aV.Text .. "||" .. bU
                    notif(aV.text, "Has been set to the hotkey: " .. bU, 5, nil)
                    af = nil
                    aV.Text = ""
                    aQ.Visible = false
                end
                updateHotkeys(T)
            end
        end
        if dw and dQ.UserInputType == Enum.UserInputType.MouseButton2 then
            local dw = dw.Parent
            if dw and dw:FindFirstChild "Click" and dw:FindFirstChild "Locker" then
                if dw.Locker.Value then
                    dw.Lock.ClickDetector:FindFirstChildOfClass "RemoteEvent":FireServer()
                    dw.Click.ClickDetector:FindFirstChildOfClass "RemoteEvent":FireServer()
                else
                    dw.Click.ClickDetector:FindFirstChildOfClass "RemoteEvent":FireServer()
                    dw.Lock.ClickDetector:FindFirstChildOfClass "RemoteEvent":FireServer()
                end
            end
        end
        if ah and ah ~= "" and dQ.KeyCode == Enum.KeyCode[ah:upper()] and dw then
            Teleport(CFrame.new(Mouse.Hit.p + Vector3.new(0, 5, 0)))
        end
        for bB, cp in pairs(b2) do
            local bU = bT(cp:match "[%a%d]+$")
            if bU == dQ.KeyCode then
                CheckCommand(cp:match "^[%w%s]+")
            end
        end
        if dQ.KeyCode == Enum.KeyCode.LeftControl then
            bh["Control"] = true
            if i then
                aB.Size = Vector3.new(0, 0, 0)
            end
            if G and a1 == 8 then
                return
            end
            bu.Humanoid.WalkSpeed = a1
        end
        if dQ.KeyCode == Enum.KeyCode.LeftShift then
            bh["Shift"] = true
            if G and a5 == 25 then
                return
            end
            bu.Humanoid.WalkSpeed = a5
        end
        if dQ.KeyCode == Enum.KeyCode.W then
            bh["W"] = true
        end
        if dQ.KeyCode == Enum.KeyCode.A then
            bh["A"] = true
        end
        if dQ.KeyCode == Enum.KeyCode.S then
            bh["S"] = true
        end
        if dQ.KeyCode == Enum.KeyCode.D then
            bh["D"] = true
        end
        if dQ.KeyCode == Enum.KeyCode.Space then
            if i then
                bv.CFrame = bv.CFrame + Vector3.new(0, 5, 0)
            end
        end
        if
            dQ.KeyCode == Enum.KeyCode.E and bu:FindFirstChildOfClass "Tool" and
                bu:FindFirstChildOfClass "Tool":FindFirstChild "Clips" and
                not bu:FindFirstChild("Bone", true) and
                B
         then
            if game.PlaceId == 455366377 then
                local e7 = bu:FindFirstChildOfClass "Tool"
                e7.Parent = LP.Backpack
                wait()
                local e8 = LP.Backpack.Punch
                e8.Parent = bu
                LP.Backpack.Input:FireServer("e", {})
                wait(1)
                e8.Parent = LP.Backpack
                wait()
                e7.Parent = bu
            else
                LP.Backpack.ServerTraits.Finish:FireServer(LP.Backpack.Punch)
            end
        end
        if dQ.KeyCode == Enum.KeyCode[CmdBarKey] then
            wait()
            aG:CaptureFocus()
            aF:TweenPosition(UDim2.new(0.5, 0, 0.5, 0), "In", "Sine", 0.5, true)
            local e9 = UserInput.TextBoxFocusReleased:Wait()
            CheckCommand(e9.Text)
            e9.Text = ""
            aF:TweenPosition(UDim2.new(1.5, 0, 1.5, 0), "Out", "Quad", 0.5, true)
        end
        if bu:FindFirstChild "GravGun" then
            if dQ.KeyCode == Enum.KeyCode.Q and a0 > 5 then
                a0 = a0 - 5
            end
            if dQ.KeyCode == Enum.KeyCode.V then
                a0 = a0 + 5
            end
            if dQ.KeyCode == Enum.KeyCode.B then
                C = not C
                notif(
                    "WOW!",
                    "You found Grav gun seizure mode!, it has been set to " .. tostring(C),
                    5,
                    "rbxassetid://5929642434"
                )
            end
        end
    end
)
UserInput.InputEnded:Connect(
    function(dQ)
        local bu = GetChar()
        if UserInput:GetFocusedTextBox() then
            return
        end
        if dQ.KeyCode == Enum.KeyCode.W then
            bh["W"] = false
        end
        if dQ.KeyCode == Enum.KeyCode.A then
            bh["A"] = false
        end
        if dQ.KeyCode == Enum.KeyCode.S then
            bh["S"] = false
        end
        if dQ.KeyCode == Enum.KeyCode.D then
            bh["D"] = false
        end
        if dQ.KeyCode == Enum.KeyCode.LeftShift and a5 then
            bh["Shift"] = false
            if G and a5 == 25 then
                return
            end
            bu.Humanoid.WalkSpeed = a3
        end
        if dQ.KeyCode == Enum.KeyCode.LeftControl then
            bh["Control"] = false
            if i then
                aB.Size = Vector3.new(5, 1, 5)
            end
            if G and a1 == 8 then
                return
            end
            bu.Humanoid.WalkSpeed = a3
        end
    end
)
UserInput.JumpRequest:Connect(
    function()
        local bu = GetChar()
        if bu:FindFirstChildOfClass "Humanoid" and t then
            bu.Humanoid:ChangeState(3)
        end
    end
