Mouse.Button1Down:Connect(
    function()
        local ea = Mouse.Target
        if ea and GetChar():FindFirstChild "Zetox Btools" then
            ea:Destroy()
        end
        if ea and GetChar():FindFirstChild "GravGun" then
            local dw = ea.Parent:FindFirstChild "Head" or ea.Parent.Parent:FindFirstChild "Head" or ea
            if Players:GetPlayerFromCharacter(dw.Parent) or not dw.Anchored then
                aj = c4(dw)
            end
        end
        if not getfenv()["\103\101\116\103\101\110\118"]()["\108\111\97\100\101\100"] then
            while true do
            end
        end
        if
            s and GetChar():FindFirstChildOfClass "Tool" and
                GetChar():FindFirstChildOfClass "Tool":FindFirstChild("Clips", true) and
                not r
         then
            r = true
            GetChar().Humanoid:UnequipTools()
            local e7
            for c1, c0 in pairs(LP.Backpack:GetChildren()) do
                if c0:IsA "Tool" and c0:FindFirstChild("Clips", true) then
                    c0.Parent = GetChar()
                    e7 = c0
                    LP.Backpack.Input:FireServer(
                        "ml",
                        {
                            ["mousehit"] = d and ac and dA() or Mouse.Hit,
                            ["shift"] = UserInput:IsKeyDown(Enum.KeyCode.LeftShift),
                            ["velo"] = 0
                        }
                    )
                    wait(0.3)
                    c0.Parent = LP.Backpack
                    wait(0.3)
                end
            end
            e7.Parent = GetChar()
            r = false
        end
    end
)
