                if bt ~= "None" then
                    cs(bt, dS)
                end
            end
        end
    end
)
RunService.Stepped:Connect(
    function()
        local bu = GetChar()
        local bv = bu:FindFirstChild "RealHumanoidRootPart" or bu:FindFirstChild "Torso"
        if I then
            local dT = bu:GetDescendants()
            for bB = 1, #dT do
                local bP = dT[bB]
                if bP:IsA "BasePart" then
                    bP.CanCollide = false
                end
            end
        end
        if
            bh["Shift"] and a5 == 25 and O and
                (LP.Backpack:FindFirstChild "ServerTraits" and LP.Backpack.ServerTraits.Stann.Value <= 5 or
                    GetChar():FindFirstChild "Stamina" and GetChar().Stamina.Value <= 5)
         then
            GetChar().Humanoid.WalkSpeed = 16
        end
        if aq and ap and ap.Character and ap.Character:FindFirstChild "Torso" then
        end
        if A and game.PlaceId ~= 455366377 then
            local dU = bu:FindFirstChild "Right Leg"
            if dU then
                dU:Destroy()
            end
        end
        local c0 = bu:FindFirstChildOfClass "Tool"
        if c and c0 and not table.find(b5, c0) then
            if bu:FindFirstChild "Right Arm" and bu["Right Arm"]:FindFirstChild "RightGrip" then
                game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
            end
        end
        if flying and bu:FindFirstChild "Humanoid" and (game.PlaceId == 455366377 and not y) then
            y = true
            LP.Character.Humanoid:ChangeState(3)
            if game.PlaceId == 455366377 then
                wait(0.2)
            end
            y = false
        end
        if ai then
            Lighting.ClockTime = ai
        end
        if i and bu:FindFirstChildOfClass "Humanoid" and bv then
            bu.Humanoid.HipHeight = 0
            aB.CFrame = bv.CFrame + Vector3.new(0, -3.5, 0)
        end
        if q and ag and ag.Character and ag.Character:FindFirstChild "Head" then
            if ag.Character:FindFirstChildOfClass "Humanoid" and ag.Character.Humanoid.Health == 0 then
                return
            end
            if ag.Character:FindFirstChild(R) then
                workspace.CurrentCamera.CoordinateFrame =
                    CFrame.new(workspace.CurrentCamera.CoordinateFrame.p, ag.Character[R].CFrame.p)
            else
                workspace.CurrentCamera.CoordinateFrame =
                    CFrame.new(workspace.CurrentCamera.CoordinateFrame.p, ag.Character.Head.CFrame.p)
            end
        end
        if w and am and am.Character and bv then
            local bt = am.Character:FindFirstChildWhichIsA("BasePart", true)
            if bt then
                bv.CFrame = bt.CFrame
            end
            local dV = LP.Backpack:GetChildren()
            for bB = 1, #dV do
                local bP = dV[bB]
                bP.Parent = bu
                bP:GetPropertyChangedSignal("Parent"):Wait()
            end
        end
        if f and ae and ae.Character and bv then
            local bt = ae.Character:FindFirstChild "Torso"
            if bt then
                if L then
                    if not x then
                        CheckCommand("fly")
                    end
                    if not a and not k then
                        CheckCommand("aimbotautoshoot")
                    end
                    if not d or ae and tostring(ac) ~= tostring(ae) then
                        CheckCommand("aim " .. ae.Name)
                    end
                    if not bu:FindFirstChildOfClass "ForceField" then
                        local dW = dD()
                        local dX = bu:FindFirstChildOfClass "Tool"
                        if typeof(dW) ~= "boolean" and dW and dW ~= dX then
                            if dX then
                                dX.Parent = LP.Backpack
                                wait()
                            end
                            dW.Parent = LP.Character
                        end
                    end
                    if ae and ae.Character and not ae.Character:FindFirstChild("Bone", true) and (not p and M or not M) then
                        if bu:FindFirstChild "Glock" or bu:FindFirstChild "Uzi" then
                            local dY = math.random(1, 6)
                            if dY <= 3 then
                                bv.CFrame = bt.CFrame * CFrame.new(math.random(1, 25), 0, math.random(1, 25))
                            elseif dY > 3 then
                                bv.CFrame = bt.CFrame * CFrame.new(-math.random(1, 25), 0, -math.random(1, 25))
                            end
                        else
                            local dY = math.random(1, 6)
                            if dY <= 3 then
                                bv.CFrame = bt.CFrame * CFrame.new(math.random(1, 15), 0, math.random(1, 15))
                            elseif dY > 3 then
                                bv.CFrame = bt.CFrame * CFrame.new(-math.random(1, 15), 0, -math.random(1, 15))
                            end
                        end
                    else
                        if not p and M or not M and not k then
                            bv.CFrame = bt.CFrame
                        end
                    end
                else
                    bv.CFrame = bt.CFrame
                end
            end
        end
        if j then
            local dZ = Players:GetPlayers()
            for bB = 1, #dZ do
                local b8 = dZ[bB]
                if
                    bv and b8 ~= LP and b8.Character and b8.Character:FindFirstChild "Head" and
                        b8.Character:FindFirstChild("Bone", true)
                 then
                    if
                        (bv.Position - b8.Character.Head.Position).magnitude < a8 and b8.Character.Humanoid.Health > 0 and
                            not b8.Character:FindFirstChild "Dragged" and
                            not table.find(b4, b8.UserId)
                     then
                        Teleport(b8.Character.Head.CFrame)
                        LP.Backpack.ServerTraits.Finish:FireServer(
                            LP.Backpack:FindFirstChild "Punch" or LP.Character:FindFirstChild "Punch"
                        )
                    end
                end
            end
        end
    end
)
