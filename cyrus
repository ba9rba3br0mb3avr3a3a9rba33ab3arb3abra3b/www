AddCommand(
    function()
        local eh = {}
        for bB = 1, 10 do
            local ei =
                HttpService:JSONDecode(
                game:HttpGet(
                    "https://www.roblox.com/games/getgameinstancesjson?placeId=" .. game.PlaceId .. "&startindex=" .. bB
                )
            )
            for bB = 1, #ei.Collection do
                local ej = ei.Collection[bB]
                eh[ej.Ping] = ej.Guid
            end
            for bB, cp in pairs(eh) do
                if cp ~= game.JobId then
                    TeleportService:TeleportToPlaceInstance(game.PlaceId, cp)
                    break
                end
            end
        end
    end,
    "serverhop",
    {},
    "Hops servers of your current game",
    "[No Args]"
)
