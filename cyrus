Players.PlayerRemoving:Connect(
    function(b8)
        if a_[tostring(b8)] then
            a_[tostring(b8)] = nil
        end
        if tostring(b8) == tostring(ac) then
            ac = nil
        end
        if b8 == am then
            w = false
            am = nil
        end
        if b8 == ae then
            L = false
            f = false
            ae = nil
            a = false
            x = false
            h = false
            ac = nil
        end
        cm(b8)
    end
)
aG:GetPropertyChangedSignal("Text"):Connect(
    function()
        pcall(
            function()
                if aG.Text ~= "" or aG.Text ~= " " then
                    local ef = 0
                    local dT = aF:GetChildren()
                    for bB = 1, #dT do
                        local bP = dT[bB]
                        if bP:IsA "TextLabel" then
                            local d6 = string.lower(bP.Text):gsub("[Alias] ", "")
                            if string.find(d6, aG.Text:lower()) then
                                bP.Position = UDim2.new(0, 0, 0, 10 + ef * 20)
                                ef = ef + 1
                                if ef >= 7 then
                                    bP.Position = UDim2.new(0, 0, 0, 1000)
                                    ef = ef - 1
                                end
                            else
                                bP.Position = UDim2.new(0, 0, 0, 1000)
                            end
                        end
                    end
                end
            end
        )
    end
)
