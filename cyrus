coroutine.resume(
    coroutine.create(
        function()
            local ey, ez, eA =
                syn_io_listdir or list_files,
                syn_io_isfolder or isfolder,
                syn_io_makefolder or makefolder
            if ey and ez and eA then
                if not ez "CyAdminPlugins" then
                    eA("CyAdminPlugins")
                end
                for c1, cp in pairs(ey "CyAdminPlugins") do
                    local eB = loadfile(cp)
                    if not eB then
                        ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(
                            "There was a syntax error (sadly can't output it as loadfile is gay)",
                            "All"
                        )
                    else
                        local bW, bG = pcall(eB)
                        if not bW then
                            ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(
                                "Runtime Error" .. bG,
                                "All"
                            )
                        end
                    end
                end
            end
            if not getfenv()["\103\101\116\103\101\110\118"]()["\108\111\97\100\101\100"] then
                while true do
                end
            end
            aD.Text = "Commands | Total Commands: " .. #aY
            for bB = 1, #aY do
                local bR = aY[bB]
                local ct, bA, eC, eD = bR.Name, bR.Args, bR.Alias, bR.Help
                da(UDim2.new(0.0008, 0, 0.002, -35 + bB * 29), ct, eD)
                coroutine.resume(
                    coroutine.create(
                        function()
                            d8(ct, bA)
                        end
                    )
                )
                coroutine.resume(
                    coroutine.create(
                        function()
                            for bB = 1, #eC do
                                d8("[Alias] " .. eC[bB], bA)
                            end
                        end
                    )
                )
            end
            while wait() do
                local bu = GetChar()
                coroutine.resume(
                    coroutine.create(
                        function()
                            if bu and bu:FindFirstChildOfClass "Humanoid" then
                                if workspace.Gravity < a9 then
                                    if game.PlaceId == 455366377 and not bu:FindFirstChild "RealHumanoidRootPart" then
                                        bu.Humanoid:ChangeState(3)
                                        bu.Humanoid.PlatformStand = false
                                        wait(0.2)
                                    end
                                    bu.Humanoid:ChangeState(8)
                                end
                                if game.PlaceId == 455366377 then
                                    if bu.Humanoid.HipHeight > 0 or i and not x then
                                        bu.Humanoid:ChangeState(3)
                                        bu.Humanoid.PlatformStand = false
                                        RunService.RenderStepped:Wait()
                                        bu.Humanoid:ChangeState(8)
                                        wait(2)
                                    end
                                end
                            end
                            local eE = tick()
                            ReplicatedStorage.DefaultChatSystemChatEvents.MutePlayerRequest:InvokeServer()
                            ao = tick() - eE
                        end
                    )
                )
                local c0 = bu:FindFirstChildOfClass "Tool"
                if
                    a and ac and c0 and c0:FindFirstChild("Clips", true) and ac:FindFirstChildOfClass "Humanoid" and
                        ac.Humanoid.Health > 0 and
                        not ac:FindFirstChildOfClass "ForceField"
                 then
                    if not dv(ac) and ck(Players:GetPlayerFromCharacter(ac), "Bone") ~= "Yes" then
                        if
                            ac:FindFirstChild "Head" and bu:FindFirstChild "Head" and
                                (ac.Head.Position - bu.Head.Position).magnitude <= bg[c0.Name]
                         then
                            if game.PlaceId == 455366377 then
                                LP.Backpack.Input:FireServer(
                                    "ml",
                                    {
                                        ["mousehit"] = dA(),
                                        ["shift"] = UserInput:IsKeyDown(Enum.KeyCode.LeftShift),
                                        ["velo"] = bu.Head.Velocity.magnitude
                                    }
                                )
                            else
                                c0.Fire:FireServer(dA())
                            end
                        end
                    end
                end
                if bu:FindFirstChildOfClass "Humanoid" then
                    if c0 and c0:FindFirstChild "Ammo" then
                        aN.Text =
                            "Health: " ..
                            math.floor(bu.Humanoid.Health) ..
                                "\nCurrent WalkSpeed: " ..
                                    math.floor(bu.Humanoid.WalkSpeed) ..
                                        "\nSprinting Speed: " ..
                                            a5 ..
                                                "\nCrouching Speed: " ..
                                                    a1 ..
                                                        "\nBlink Speed: " ..
                                                            W ..
                                                                "\nJumpPower: " ..
                                                                    bu.Humanoid.JumpPower ..
