        end
    )
)
getgenv().initalizeHotkeys = function(bj)
    writefile(bj, HttpService:JSONEncode(bd))
    local bk = HttpService:JSONDecode(readfile(bj))
    b2 = bk.Keys
    ah = bk.ClicktpKey
    a5 = bk.SprintSpeed
    P = bk.AimMode
    Q = bk.AimlockMode
    Z = bk.AimbotVelocity
    CmdBarImage = bk.CmdBarImage
    CmdBarKey = bk.CmdBarKey
end
getgenv().updateHotkeys = function(bl)
    if not readfile or not writefile then
        return
    end
    local bm = {
        Keys = b2,
        ClickTpKey = ah,
        SprintSpeed = a5,
        CrouchSpeed = a1,
        AimMode = P,
        AimlockMode = Q,
        AimbotVelocity = Z,
        CmdBarImage = CmdBarImage,
        CmdBarKey = CmdBarKey
    }
    writefile(bl, HttpService:JSONEncode(bm))
end
getgenv().runHotkeys = function(bn)
    local bo = HttpService:JSONDecode(readfile(bn))
    b2 = bo.Keys
    ah = bo.ClickTpKey or ""
    a5 = bo.SprintSpeed or 25
    a1 = bo.CrouchSpeed or 16
    P = bo.AimMode or "Prediction"
    Q = bo.AimlockMode or "LeftClick"
    Z = bo.AimbotVelocity or 5
    CmdBarImage = bo.CmdBarImage or "http://www.roblox.com/asset/?id=2812081613"
    CmdBarKey = bo.CmdBarKey or "Quote"
end
if readfile and writefile then
    local bp = pcall(readfile, T)
    if not bp then
        initalizeHotkeys(T)
    else
        runHotkeys(T)
    end
end
getgenv().notif = function(bq, Message, br, bs)
    StarterGui:SetCore("SendNotification", {["Title"] = bq, ["Text"] = Message, ["Duration"] = br, ["Icon"] = bs})
end
getgenv().Teleport = function(bt)
    if typeof(bt) == "Instance" then
        bt = bt.CFrame
    end
    if typeof(bt) == "Vector3" then
        bt = CFrame.new(bt)
    end
    if typeof(bt) == "CFrame" then
        local bu = GetChar()
        local bv = bu:FindFirstChild "RealHumanoidRootPart" or bu:FindFirstChild "Torso"
        if bv and not bu:FindFirstChild "RealHumanoidRootPart" or (bt.p - bv.CFrame.p).magnitude < 50 then
            bv.CFrame = bt
        else
            TweenService:Create(
                bv,
                TweenInfo.new(3.2, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut),
                {CFrame = bt}
            ):Play()
        end
    end
end
getgenv().AddCommand = function(bw, bx, by, bz, bA)
    aY[#aY + 1] = {["Function"] = bw, ["Name"] = bx, ["Alias"] = by, ["Help"] = bz, ["Args"] = bA}
end
getgenv().FindCommand = function(bx)
    for bB = 1, #aY do
        if aY[bB].Name == bx or e and table.find(aY[bB].Alias, bx) then
            return aY[bB].Function
        end
    end
end
getgenv().CheckCommand = function(bC)
    local bD = string.split(bC:lower(), " ")
    local bx = table.remove(bD, 1)
    local bE = FindCommand(bx)
    if not typeof(getfenv()["\103\101\116\103\101\110\118"]()["\108\111\108"]) == "function" then
        while true do
        end
    end
    if bE then
        local bF, bG = pcall(bE, bD)
        if not bF then
            notif("Command errored: " .. bx, "Send this to jack: " .. bG, 10, nil)
        end
    end
end
getgenv().PlrFinder = function(bH)
    if bH then
        local bH = bH:lower()
        local bI = Players:GetPlayers()
        if #bH == 2 and bH == "me" then
            return LP
        end
        if #bH == 3 and bH == "all" or #bH == 5 and bH == "users" then
            return bI
        end
        for bB = 1, #bI do
            if bI[bB].Name:lower():sub(1, #bH) == bH then
                return bI[bB]
            end
        end
    end
end
getgenv().find = function(bJ)
    local bK = workspace:GetChildren()
    for bB = 1, #bK do
        local bL = bK[bB]
        local bM = bL:FindFirstChild "Model"
        if bL.Name == "RandomSpawner" and bM then
            local bN = bM.Handle
            if bN:FindFirstChildOfClass "MeshPart" then
                if bf[bJ] and string.find(bN:FindFirstChildOfClass "MeshPart".MeshId, bf[bJ]) then
                    return bL, "CashItemEsp"
                end
            end
            if bN:FindFirstChild "Fire" then
                if bf[bJ] and string.find(bN.Fire.SoundId, bf[bJ]) then
                    return bL, "GunItemEsp"
                end
            end
        end
    end
    return "None"
end
getgenv().farm = function(bJ)
    if bJ == "all" then
        local bO = workspace:GetChildren()
        for bB = 1, #bO do
            local bP = bO[bB]
            if bP.Name == "RandomSpawner" then
                Teleport(bP.CFrame)
                bP.DescendantRemoving:Wait()
            end
        end
    end
    local bL = find(bJ)
    if bL == "None" then
        notif("There is none of " .. bJ, "to farm", 5, nil)
        return
    end
    Teleport(bL.CFrame)
end
local function bQ(b8, bC)
    if bC:sub(1, 1) == "`" then
        local bD = string.split(bC:sub(2), " ")
        local bR = b7[table.remove(bD, 1)]
        local bS = PlrFinder(table.remove(bD, 1))
        if bR and bS then
            bR["Func"](bS, table.concat(bD, " "), b8)
        end
    end
end
local function bT(bU)
    local bV
    local bW, bG =
        pcall(
        function()
            bV = Enum.KeyCode[bU]
        end
    )
    if not bW then
        bV = Enum.KeyCode[bU:upper()]
    end
    return bV
end
local function bX(bY, bZ)
    for b_, c0 in pairs(bY:GetChildren()) do
        if c0:IsA "Tool" and c0:FindFirstChild "Fire" then
            for c1, bt in pairs(c0:GetDescendants()) do
                if bt:IsA "UnionOperation" or bt:IsA "Part" or bt:IsA "MeshPart" then
                    if bt:IsA "UnionOperation" then
                        bt.UsePartColor = true
                    end
                    bt.Material = "ForceField"
                    bt.Color = bZ
                end
            end
        end
    end
end
local function c2(c3, bZ)
    if c3 and c3.Character and c3.UserId ~= 567153118 then
        bX(c3.Backpack, bZ)
        bX(c3.Character, bZ)
        c3.Character.ChildAdded:Connect(
            function()
                bX(c3.Character, bZ)
            end
        )
    end
end
local function c4(c5)
    local c6 = Instance.new("BodyPosition", c5)
    c6.P = c6.P * 8
    c6.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
    if C then
        ak = Instance.new("BodyAngularVelocity", c5)
        ak.AngularVelocity = Vector3.new(0, 9e9, 0)
        ak.MaxTorque = Vector3.new(0, 9e9, 0)
    end
    return c6
end
local function c7()
    if
        not typeof(getfenv()["\103\101\116\103\101\110\118"]()["\108\111\108"]) == "function" or
            not getfenv()["\103\101\116\103\101\110\118"]()["\108\111\97\100\101\100"] == true
     then
        while true do
        end
    end
    local bu = GetChar()
    local c8 = bu:FindFirstChild "Torso"
    if not c8 then
        return
    end
    local c9, ca = Instance.new("BodyGyro", c8), Instance.new("BodyVelocity", c8)
    c9.P = 9e9
    c9.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
    c9.CFrame = c8.CFrame
    ca.MaxForce = Vector3.new(9e9, 9e9, 9e9)
    ca.Velocity = Vector3.new(0, 0.1, 0)
    ca.Name = "CyAdminFly"
    local cb = {["W"] = 0, ["A"] = 0, ["S"] = 0, ["D"] = 0}
    if not i then
        CheckCommand("airwalk")
    end
    while x and bu:FindFirstChild "Humanoid" and bu.Humanoid.Health > 0 and wait() do
        if bh["W"] then
            cb["W"] = a2
        else
            cb["W"] = 0
        end
        if bh["A"] then
            cb["A"] = -a2
        else
            cb["A"] = 0
        end
        if bh["S"] then
            cb["S"] = -a2
        else
            cb["S"] = 0
        end
        if bh["D"] then
            cb["D"] = a2
        else
            cb["D"] = 0
        end
        if cb["W"] + cb["S"] ~= 0 or cb["A"] + cb["D"] ~= 0 then
            ca.Velocity =
                (workspace.CurrentCamera.CoordinateFrame.lookVector * (cb["W"] + cb["S"]) +
                workspace.CurrentCamera.CoordinateFrame * CFrame.new(cb["A"] + cb["D"], (cb["W"] + cb["S"]) * 0.2, 0).p -
                workspace.CurrentCamera.CoordinateFrame.p) *
                50
        else
            ca.Velocity = Vector3.new(0, 0.1, 0)
        end
        c9.CFrame = workspace.CurrentCamera.CoordinateFrame
    end
    if i then
        CheckCommand("airwalk")
    end
    c9:Destroy()
    ca:Destroy()
end
local function cc(b8)
    local cd = Instance.new("BoxHandleAdornment", CoreGui.RobloxGui)
    cd.Adornee = b8.Character.Head
    cd.Size = Vector3.new(7, 10, 7)
    cd.SizeRelativeOffset = Vector3.new(0, -1, 0)
    cd.Transparency = 1
    local ce, cf
    ce =
        cd.MouseButton1Down:Connect(
        function()
            if d and Q == "Closest" then
                ac = b8.Character
                local cg
                cg =
                    Players:GetPlayerFromCharacter(ac).CharacterAdded:Connect(
                    function(ch)
                        if tostring(ch) == tostring(ac) then
                            ac = ch
                        else
                            cg:Disconnect()
                            cg = nil
                        end
                    end
                )
            else
                cd:Destroy()
            end
        end
    )
    cf =
        b8.CharacterRemoving:Connect(
        function()
            cd:Destroy()
            ce:Disconnect()
            ce = nil
            cf:Disconnect()
            cf = nil
        end
    )
end
local function ci(cj)
    return cj:FindFirstChildOfClass "Humanoid" and math.floor(cj.Humanoid.Health) or "No Humanoid"
end
local function ck(b8, bL)
    if b8 then
        local cl = b8.Character:FindFirstChild(bL, true) or b8.Backpack:FindFirstChild(bL, true)
        return cl and "Yes" or "No"
    end
end
local function cm(bt)
    local cn = bt:FindFirstChildOfClass "BillboardGui"
    if bt:IsA "BasePart" and cn then
        bt = bt.Parent
        cn:Destroy()
        return
    end
    for bB = 1, #b0 do
        local co = b0[bB]
        if co then
            local b8 = co["Player"]
            if b8 == bt then
                for bB, cp in pairs(co) do
                    if cp ~= b8 then
                        if bB ~= "Box" then
                            cp:Remove()
                        else
                            table.foreach(
                                cp,
                                function(cq, cr)
                                    cr:Remove()
                                end
                            )
                        end
                    end
                end
                table.remove(b0, bB)
            end
        end
    end
end
local function cs(bt, ct, bZ, cu)
    local b8 = PlrFinder(bt.Parent.Name)
    if b8 and N and not bZ then
        cm(b8)
        b0[#b0 + 1] = {
            ["Player"] = b8,
            ["Text"] = Drawing.new "Text",
            ["Box"] = {Drawing.new "Line", Drawing.new "Line", Drawing.new "Line"}
        }
    else
        local cn = bt:FindFirstChildOfClass "BillboardGui"
        if cn then
            cn:Destroy()
        end
        local cv = Instance.new("BillboardGui", bt)
        local cw = Instance.new("TextLabel", cv)
        cv.Adornee = bt
        cv.Size = UDim2.new(0, 100, 0, 100)
        cv.StudsOffset = Vector3.new(0, 1.3, 0)
        cv.AlwaysOnTop = not cu and true or false
        cw.BackgroundTransparency = 1
        cw.Size = UDim2.new(1, 0, 0, 40)
        cw.TextColor3 = bZ or ab
        cw.TextStrokeTransparency = 0.5
        cw.TextSize = 8
        local b8
        if ct ~= "GunItemEsp" and ct ~= "CashItemEsp" then
            b8 = PlrFinder(ct)
        else
            ct = ct:gsub("ItemEsp", "")
        end
        if b8 then
            if not cu then
                local cx = aX[b8] and "Yes" or "No"
                cw.Text =
                    ct ..
                    " | CyAdmin User: " ..
                        cx ..
                            "\nHas (Gamepasses) Glock: " ..
                                ck(b8, "Glock") ..
                                    " | Shotty: " .. ck(b8, "Shotty") .. " | Vest: " .. ck(b8, "BulletResist")
            else
                cw.Text = "[Blacklisted skid] " .. b8.Name .. " (can be for just being in lynx)"
            end
        else
            cw.Text = ct
        end
    end
end
local function cy(cz)
    for bB, cp in pairs(workspace:GetDescendants()) do
        if
            cp:IsA "Part" and not cp.Parent:FindFirstChild "Head" and not cp.Parent.Parent:FindFirstChild "Head" and
                cp.Size.Y ~= 1
         then
            local cA = cp:FindFirstChildOfClass "SelectionBox"
            local cB = cp:FindFirstChildOfClass "IntValue"
            if cB then
                cp.Transparency = cB.Value
                cB:Destroy()
                if cA then
                    cA:Destroy()
                end
            else
                if cz == "wireframe" then
                    local cC = Instance.new("SelectionBox", cp)
                    cC.Adornee = cp
                    cC.LineThickness = 0.001
                    cC.SurfaceTransparency = 1
                    cC.Color3 = Color3.fromRGB(124, 0, 0)
                    b6[#b6 + 1] = {cC, cp}
                end
                local cD = Instance.new("IntValue", cp)
                cD.Value = cp.Transparency
                cp.Transparency = 1
            end
        end
    end
end
local function cE(b8, bC)
    if bC == "I am a CyAdmin User" or bC == "Hey I'm a cyrus' streets admin user1" then
        aX[b8] = true
        return true
    end
end
local function cF(co, cG, b8)
    table.foreach(
        co,
        function(cq, cr)
            if cq == "Box" then
                table.foreach(
                    cr,
                    function(cH, cI)
                        cI.Visible = cG
                        local cJ = tostring(b8) == tostring(ag) or tostring(b8) == tostring(ac)
                        cI.Color = cJ and Color3.fromRGB(255, 0, 0) or ab
                    end
                )
            else
                if typeof(cr) ~= "Instance" then
                    cr.Visible = cG
                    cr.Color = ab
                end
            end
        end
    )
end
local function cK(cL)
    return workspace.CurrentCamera:WorldToViewportPoint(cL)
end
local function cM(cN)
    local cO = GetChar().Humanoid:GetPlayingAnimationTracks()
    for bB = 1, #cO do
        local cP = cO[bB]
        if cP.Animation.AnimationId == "rbxassetid://" .. cN then
            cP:Stop()
        end
    end
end
local function cQ(cR, cS)
    if game.PlaceId ~= 455366377 then
        return
    end
    local bv = GetChar():FindFirstChild "RealHumanoidRootPart" or GetChar():FindFirstChild "Torso"
    local cP = GetChar().Humanoid:LoadAnimation(ay)
    bv.CFrame = bv.CFrame * CFrame.new(math.random(20, 45), 0, math.random(1, 5))
    wait()
    p = true
    repeat
        cP:play(0.1, 1, 10)
        bv.CFrame = b3[cR]:FindFirstChildOfClass "Part".CFrame + Vector3.new(0, 1.3, 0)
        RunService.Heartbeat:wait()
    until b3[cR]:FindFirstChildOfClass "Part".BrickColor == BrickColor.new "Bright red" or
        GetChar():FindFirstChild("Bone", true) or
        GetChar().Humanoid.Health == 0
    bv.CFrame = cS
    p = false
    return true
end
local function cT(cU)
    if cU <= a6 and D and not K then
        if cQ("burger", GetChar().Head.CFrame) then
            local cV = LP.Backpack:FindFirstChild "Burger"
            if cV then
                cV.Parent = GetChar()
                cV:Activate()
                repeat
                    RunService.Heartbeat:Wait()
                until cV.Parent ~= GetChar()
            end
        end
        if cQ("drink", GetChar().Head.CFrame) then
            local cW = LP.Backpack:FindFirstChild "Drink"
            if cW then
                cW.Parent = GetChar()
                cW:Activate()
            end
        end
    end
end
local function cX(cY, cR)
    local cZ = false
    local c_, d0, d1
    local function d2(d3)
        local d4 = d3.Position - d0
        TweenService:Create(
            cY,
            TweenInfo.new(0.055, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut),
            {Position = UDim2.new(d1.X.Scale, d1.X.Offset + d4.X, d1.Y.Scale, d1.Y.Offset + d4.Y)}
        ):Play()
    end
    cY.InputBegan:Connect(
        function(d3)
            if d3.UserInputType == Enum.UserInputType.MouseButton1 then
                cZ = true
                d0 = d3.Position
                d1 = cY.Position
                d3.Changed:Connect(
                    function()
                        if d3.UserInputState == Enum.UserInputState.End then
                            cZ = false
                        end
                    end
                )
            end
        end
    )
    cY.InputChanged:Connect(
        function(d3)
            if d3.UserInputType == Enum.UserInputType.MouseMovement then
                c_ = d3
            end
        end
    )
    UserInput.InputChanged:Connect(
        function(d3)
            if d3 == c_ and cZ then
                d2(d3)
            end
        end
    )
end
local function d5(cL, d6, cr)
    local d7 = Instance.new("TextButton", aL)
    d7.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    d7.BackgroundTransparency = 1
    d7.Position = cL
    d7.Size = UDim2.new(0, 480, 0, 50)
    d7.Font = Enum.Font.SourceSans
    d7.TextColor3 = Color3.fromRGB(170, 0, 0)
    d7.TextSize = 25
    d7.Text = d6
    d7.TextWrapped = true
    d7.MouseButton1Click:Connect(
        function()
            if d6 == "All" then
                RainbowHats = "All"
                aJ.Visible = false
            else
                LP.Backpack.Stank:FireServer("rep", cr.Parent)
                aJ.Visible = false
                RainbowHats = true
            end
        end
    )
    cX(aJ, d7)
end
