local function dj(dk)
    if dk:IsA "Trail" then
        dk.Color = aa
    end
    if s and dk:FindFirstChild("Clips", true) then
        for c1, cp in pairs(LP.Backpack:GetChildren()) do
            if cp:IsA "Tool" and cp:FindFirstChild("Clips", true) then
                cp.Parent = GetChar()
            end
        end
    end
    if not s and bg[dk.Name] and U ~= "None" then
        wait()
        if dk.Name ~= "Shotty" and dk.Name ~= "Sawed Off" or U == "1" then
            GetChar().Humanoid:LoadAnimation(az):Play()
        else
            local cP = GetChar().Humanoid:LoadAnimation(aA)
            cP:Play()
            wait()
            cP:AdjustSpeed(0)
        end
    end
    if dk.Name == "Bone" then
        if h then
            GetChar().Humanoid.Health = 0
        end
        if A and game.PlaceId == 455366377 then
            for bB, cp in pairs(GetChar():GetDescendants()) do
                if cp:IsA "NumberValue" then
                    cp:Destroy()
                end
            end
        end
    end
    if dk:IsA "ObjectValue" and dk.Name == "creator" then
        local b8 = dk.Value
        if m then
            if d then
                ac = b8
                local cg
                cg =
                    Players:GetPlayerFromCharacter(ac).CharacterAdded:Connect(
                    function(ch)
                        if tostring(ch) == tostring(ac) then
                            ac = ch
                        else
                            cg:Disconnect()
                            cg = nil
                        end
                    end
                )
            end
            if q then
                ag = Players:GetPlayerFromCharacter(b8)
            end
        end
        if l then
            CheckCommand("feloop " .. tostring(b8))
        end
        if k and not L then
            CheckCommand("triggerbot " .. tostring(b8))
            j = true
            local dl
            dl =
                Players:GetPlayerFromCharacter(b8).CharacterRemoving:Connect(
                function(dm)
                    if tostring(dm) == tostring(ae) then
                        L = false
                        f = false
                        ae = nil
                        a = false
                        x = false
                        h = false
                        j = false
                        ac = nil
                        dl:Disconnect()
                    else
                        dl:Disconnect()
                    end
                end
            )
        end
        pcall(
            function()
                local c0, dn = dh(b8)
                di(
                    b8.Name ..
                        " has " ..
                            dn ..
                                " from " ..
                                    math.floor((GetChar().Head.Position - b8.Head.Position).magnitude) ..
                                        " studs with a " .. c0.Name
                )
            end
        )
    end
end
