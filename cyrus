local function dq(dr)
    if not GetChar():FindFirstChild "Head" then
        return
    end
    if workspace:FindFirstChild "FreecamPart" then
        workspace.FreecamPart:Destroy()
    end
    dr = dr or 35
    GetChar().Head.Anchored = true
    local ds = Instance.new("Part", workspace)
    ds.Name = "FreecamPart"
    ds.Position = GetChar().Head.Position + Vector3.new(0, 5, 0)
    ds.Transparency = 1
    ds.CanCollide = false
    ds.Anchored = true
    workspace.CurrentCamera.CameraSubject = ds
    while z and GetChar().Humanoid.Health > 0 and wait() do
        local cL = Vector3.new()
        local dt = (workspace.CurrentCamera.Focus.p - workspace.CurrentCamera.CoordinateFrame.p).unit
        local du = ds.Position
        if bh["w"] then
            cL = cL + Vector3.new(0, 0, -1)
        elseif bh["a"] then
            cL = cL + Vector3.new(-1, 0, 0)
        elseif bh["s"] then
            cL = cL + Vector3.new(0, 0, 1)
        elseif bh["d"] then
            cL = cL + Vector3.new(1, 0, 0)
        elseif bh["Space"] then
            cL = cL + Vector3.new(0, 1, 0)
        elseif bh["Control"] then
            cL = cL + Vector3.new(0, -1, 0)
        end
        ds.CFrame = CFrame.new(du, du + dt) * CFrame.new(cL * dr)
    end
    workspace.CurrentCamera.CameraSubject = GetChar()
    GetChar().Head.Anchored = false
    if workspace:FindFirstChild "FreecamPart" then
        workspace.FreecamPart:Destroy()
    end
end
local function dv(dw)
    if dw:FindFirstChild "Head" and GetChar():FindFirstChild "Head" then
        local dx = Ray.new(dw.Head.Position, GetChar().Head.Position)
        local dy = workspace:FindPartOnRay(dx)
        if dy then
            return dy:IsDescendantOf(dw)
        end
    end
end
local function dz()
    if game.PlaceId == 455366377 then
        if
            bh["Shift"] and
                (O and
                    (LP.Backpack:FindFirstChild "ServerTraits" and LP.Backpack.ServerTraits.Stann.Value > 0 or
                        GetChar():FindFirstChild "Stamina" and GetChar().Stamina.Value > 0) or
                    not O)
         then
            if G and a5 == 25 then
                return
            end
            GetChar().Humanoid.WalkSpeed = a5
            return
        end
        if bh["Control"] then
            if G and a1 == 8 then
                return
            end
            GetChar().Humanoid.WalkSpeed = a1
            return
        end
        if not O then
            GetChar().Humanoid.WalkSpeed = a3
        end
    end
end
local function dA()
    local dB
    local dC = ac.FindFirstChild(ac, "RealHumanoidRootPart") or ac.FindFirstChild(ac, "Torso")
    if dC and P == "OldPrediction" then
        dB = dC.CFrame + dC.Velocity / Z
    elseif dC and P == "Prediction" then
        dB = dC.CFrame + dC.Velocity / (ao < 0.26 and 5 or 7.5) + dC.RotVelocity / (ao < 0.26 and 5 or 7.5)
    elseif ac.FindFirstChild(ac, P) then
        dB = ac[P].CFrame
    end
    return dB
end
local function dD()
    local dE
    local c0 = LP.Character:FindFirstChildOfClass "Tool"
    if c0 and bg[c0.Name] and (c0.Ammo.Value > 0 or c0.Clips.Value > 0) then
        return true
    else
        for bB, cp in pairs(LP.Backpack:GetChildren()) do
            if cp:IsA "Tool" and bg[cp.Name] then
                if cp:FindFirstChild "Clips" then
                    if cp.Clips.Value > 0 or cp.Ammo.Value > 0 then
                        dE = cp
                        break
                    end
                end
            end
        end
        if dE then
            return dE
        else
            if
                game.PlaceId == 455366377 and GetChar():FindFirstChildOfClass "Tool" and
                    GetChar():FindFirstChildOfClass "Tool":FindFirstChild "Ammo" and
                    M and
                    not p and
                    tonumber(LP.PlayerGui.HUD.Cash.Text:sub(2)) >= 25
             then
                cQ("ammo", GetChar().Head.CFrame)
                return true
            else
                GetChar():BreakJoints()
            end
            return false
        end
    end
end
local dF = getrawmetatable(game)
setreadonly(dF, false)
local newcclosure, getnamecallmethod, checkcaller, dG = newcclosure, getnamecallmethod, checkcaller, getcallingscript
local dH, dI = {"WalkSpeed", "JumpPower", "HipHeight", "Health"}, {"bv", "hb", "ws"}
local dJ, dK
do
    local dL, dM = game.IsA, game.IsDescendantOf
    local dN = table.find
    local unpack = unpack
    local StarterGui = game:GetService "StarterGui"
    local dO = StarterGui.SetCore
    local dP = function(self, dQ, cr)
        if not checkcaller() then
            if dL(self, "Humanoid") then
                dO(StarterGui, "ResetButtonCallback", true)
                if dN(dH, dQ) then
                    return
                end
            end
            if self == workspace and dQ == "Gravity" then
                return a9
            end
            if dQ == "CFrame" and dM(self, LP.Character) then
                return
            end
        end
        return dJ(self, dQ, cr)
    end
    local dR = function(self, ...)
        local dn = getnamecallmethod()
        local bA = {...}
        if checkcaller() then
            if dn == "FindFirstChild" and bA[1] == "RealHumanoidRootPart" then
                bA[1] = "HumanoidRootPart"
            end
            return dK(self, unpack(bA))
        end
        if (dn == "Destroy" or dn == "Kick") and (self == LP or dL(self, "BodyMover")) then
            return wait(9e9)
        end
        if dn == "BreakJoints" and self == LP.Character then
            return wait(9e9)
        end
        if (dn == "WaitForChild" or dn == "FindFirstChild") and dG() ~= script and K and bA[1] == "HumanoidRootPart" then
            bA[1] = "Torso"
            return dK(self, unpack(bA))
        end
        if dn == "FireServer" then
            if dN(dH, bA[1]) then
                return wait(9e9)
            end
            local ct = self.Name
            if ct == "Fire" and d and ac then
                return dK(self, dA())
            end
            if ct == "Input" then
                if dN(dI, bA[1]) then
                    return wait(9e9)
                end
                if d and ac then
                    if bA[2] and bA[2].mousehit then
                        bA[2].mousehit = dA()
                        return dK(self, unpack(bA))
                    end
                end
            end
            if self.Parent == ReplicatedStorage or bA[1] == "hey" and ct ~= "SayMessageRequest" then
                return wait(9e9)
            end
            if ct == "Touch1" and g then
                bA[3] = true
                return dK(self, unpack(bA))
            end
            if bA[1] == "play" then
                an = bA[2]
            elseif bA[1] == "stop" then
                an = nil
            end
        end
        return dK(self, unpack(bA))
    end
    if syn then
        dJ = hookmetamethod(game, "__newindex", newcclosure(dP))
        dK = hookmetamethod(game, "__namecall", newcclosure(dR))
    else
        dJ = hookfunction(dF.__newindex, newcclosure(dP))
        dK = hookfunction(dF.__namecall, newcclosure(dR))
    end
end
