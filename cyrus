                    else
                        aN.Text =
                            "Health: " ..
                            math.floor(bu.Humanoid.Health) ..
                                "\nCurrent WalkSpeed: " ..
                                    math.floor(bu.Humanoid.WalkSpeed) ..
                                        "\nSprinting Speed: " ..
                                            a5 ..
                                                "\nCrouching Speed: " ..
                                                    a1 ..
                                                        "\nBlink Speed: " ..
                                                            W ..
