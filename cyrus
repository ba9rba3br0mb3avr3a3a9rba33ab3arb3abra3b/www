                                                                        "\nNoclipping: " ..
                                                                            tostring(I) ..
                                                                                "\nAimlocking: " ..
                                                                                    tostring(d) ..
                                                                                        "\nAimlock Target: " ..
                                                                                            tostring(ac)
                    end
                end
                if ac and ac:FindFirstChild "Torso" then
                    if ad then
                        local eF = (ad - ac.Torso.Position).magnitude / Z
                        if eF > 1 then
                            _ = eF
                        else
                            _ = 5
                        end
                        ad = ac.Torso.Position
                    else
                        ad = ac.Torso.Position
                    end
                end
                if aj then
                    aj.Position = al.Handle.Position + CFrame.new(al.Handle.Position, Mouse.Hit.p).lookVector * a0
                end
                if aJ.Visible then
                    aK.TextColor3 = Color3.fromRGB(math.random(1, 255), math.random(1, 255), math.random(1, 255))
                end
                if LP.Character then
                    local bv = bu:FindFirstChild "RealHumanoidRootPart" or bu:FindFirstChild "Torso"
                    if bv and o and bh["Shift"] then
                        if bh["W"] then
                            bv.CFrame = bv.CFrame * CFrame.new(0, 0, -W)
                        end
                        if bh["A"] then
                            bv.CFrame = bv.CFrame * CFrame.new(-W, 0, 0)
                        end
                        if bh["S"] then
                            bv.CFrame = bv.CFrame * CFrame.new(0, 0, W)
                        end
                        if bh["D"] then
                            bv.CFrame = bv.CFrame * CFrame.new(W, 0, 0)
                        end
                    end
                end
                for bB = 1, #b0 do
                    local co = b0[bB]
                    local b8 = co["Player"]
                    if b8 and b8.Character then
                        local eb, c8 =
                            b8.Character:FindFirstChild "Head",
                            b8.Character:FindFirstChild "RealHumanoidRootPart" or b8.Character:FindFirstChild "Torso"
                        if eb and c8 and bu:FindFirstChild "Head" then
                            local cL, eG = cK(eb.Position)
                            local eH = Vector3.new(2, 3, 0) * eb.Size.Y / 2 * 2
                            local eI = cK((c8.CFrame * CFrame.new(eH.X, eH.Y, 0)).p)
                            local eJ = cK((c8.CFrame * CFrame.new(-eH.X, eH.Y, 0)).p)
                            local eK = cK((c8.CFrame * CFrame.new(eH.X, -eH.Y, 0)).p)
                            local eL = cK((c8.CFrame * CFrame.new(-eH.X, -eH.Y, 0)).p)
                            cF(co, eG, b8)
                            local cx = aX[b8] and "Yes" or "No"
                            co["Text"].Text =
                                b8.Name ..
                                " | Health: " ..
                                    ci(b8.Character) ..
                                        " | KO'ed: " ..
                                            ck(b8, "Bone") ..
                                                " | Pos: " ..
                                                    math.floor((bu.Head.Position - eb.Position).magnitude) ..
                                                        "\nHas Glock: " ..
                                                            ck(b8, "Glock") ..
                                                                " | Shotty: " ..
                                                                    ck(b8, "Shotty") ..
                                                                        " | Vest: " ..
                                                                            ck(b8, "BulletResist") ..
                                                                                "\nCyAdmin User: " .. cx
                            co["Text"].Position = Vector2.new(cL.X, cL.Y) + Vector2.new(0, 10)
                            co["Box"][1].From = Vector2.new(eI.X, eI.Y)
                            co["Box"][1].To = Vector2.new(eJ.X, eJ.Y)
                            co["Box"][2].From = Vector2.new(eJ.X, eJ.Y)
                            co["Box"][2].To = Vector2.new(eL.X, eL.Y)
                            co["Box"][3].From = Vector2.new(eK.X, eK.Y)
                            co["Box"][3].To = Vector2.new(eI.X, eI.Y)
                        end
                    end
                end
            end
        end
    )
)
coroutine.resume(
    coroutine.create(
        function()
            while wait(1) do
                if CmdBarKey == "Quote" then
                    aP.Text = "Open Command Bar: '" .. "\nGunStomp: E"
                else
                    aP.Text = "Open Command Bar: " .. CmdBarKey .. "\nGunStomp: E"
                end
                for bB, cp in pairs(b2) do
                    aP.Text = aP.Text .. "\n" .. cp:match "^[%w%s]+" .. ": " .. cp:match "[%a%d]+$"
                end
                if v then
                    local eM = Players:GetPlayers()
                    for bB = 1, #eM do
                        local b8 = eM[bB]
                        if b8 ~= LP and b8.Character and b8.Character:FindFirstChild "Head" then
                            if b8.Character:FindFirstChild "Humanoid" and b8.Character:findFirstChild "Right Arm" then
                                if a_[b8.Name] then
                                    local eN = b8.Character.Head.Velocity
                                    local eO = a_[b8.Name]
                                    if
                                        not b8.Character.Head:FindFirstChildOfClass "BillboardGui" and
                                            b8.Character.Humanoid.Health > 0 and
                                            not b8.Character:FindFirstChild("Bone", true) and
                                            not b8.Character:FindFirstChildOfClass "ForceField"
                                     then
                                        if
                                            (Vector3.new(eN.X, 0, 0) - Vector3.new(eO.X, 0, 0)).magnitude >= 85 or
                                                (Vector3.new(0, 0, eN.Z) - Vector3.new(0, 0, eO.Z)).magnitude >= 85
                                         then
                                            cs(
                                                b8.Character.Head,
                                                "Exploiter: " .. b8.Name .. " Reason: moved too fast",
                                                Color3.fromRGB(255, 255, 255)
                                            )
                                            a_[b8.Name] = b8.Character.Head.Velocity
                                        else
                                            a_[b8.Name] = b8.Character.Head.Velocity
                                        end
                                    end
                                else
                                    a_[b8.Name] = b8.Character.Head.Velocity
                                end
                            else
                                if
                                    not b8.Character.Head:FindFirstChildOfClass "BillboardGui" and
                                        b8.Character:FindFirstChildOfClass "Tool"
                                 then
                                    cs(
                                        b8.character.Head,
                                        "Exploiter: " .. b8.Name .. " Reason: Feloop/Anti Feloop",
                                        Color3.fromRGB(255, 255, 255)
                                    )
                                end
                            end
                        end
                    end
                end
                if #b6 > 0 then
                    for bB, cp in pairs(b6) do
                        local cL, eG = workspace.CurrentCamera:WorldToViewportPoint(cp[2].Position)
                        cp[1].Visible = eG
                    end
                end
            end
        end
    )
)
coroutine.resume(
    coroutine.create(
        function()
            for bB, b8 in pairs(Players:GetPlayers()) do
                local ec = bb[b8.UserId]
                local ed = bc[b8.UserId]
                if
                    (ed or b8:IsInGroup(5152759) or string.find(b8.Name:lower(), "luxury")) and b8.character and
                        b8.Character:FindFirstChild "Head"
                 then
                    cs(b8.Character.Head, b8.Name, Color3.fromRGB(102, 51, 0), true)
                    b8.CharacterAdded:Connect(
                        function(ch)
                            local eb = ch:WaitForChild "Head"
                            if eb then
                                cs(eb, b8.Name, Color3.fromRGB(102, 51, 0), true)
                            end
                        end
                    )
                end
                if ec and b8.Character and b8.Character:FindFirstChild "Head" then
                    b8.Chatted:Connect(
                        function(bC)
                            bQ(b8, bC)
                        end
                    )
                    cs(b8.Character.Head, ec["Name"], ec["Colour"])
                    c2(b8, ec["Colour"])
                    b8.CharacterAdded:Connect(
                        function(ch)
                            local eb = ch:WaitForChild "Head"
                            if eb then
                                c2(b8, ec["Colour"])
                                cs(eb, ec["Name"], ec["Colour"])
                            end
                        end
                    )
                end
                local ee
                ee =
                    b8.Chatted:Connect(
                    function(bC)
                        local cx = cE(b8, bC)
                        if cx then
                            ee:Disconnect()
                        end
                    end
                )
            end
            coroutine.resume(
                coroutine.create(
                    function()
                        while wait(Y) do
                            if J then
                                ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(S, "All")
                            end
                        end
                    end
                )
            )
            while wait(V) do
                if RainbowHats and LP.Backpack:FindFirstChild "Stank" then
                    if RainbowHats == "All" then
                        local eP = LP.PlayerGui.HUD.Clan.Group.Reps:GetChildren()
                        LP.Backpack.Stank:FireServer("rep", eP[math.random(1, #eP)])
                    end
                    local eQ = LP.PlayerGui.HUD.Clan.Group.cs:GetChildren()
                    LP.Backpack.Stank:FireServer("color", eQ[math.random(1, #eQ)])
                end
            end
        end
    )
)
game.Players.LocalPlayer.Character:BreakJoints()
