Players.PlayerAdded:Connect(
    function(b8)
        if bb[b8.UserId] then
            print(1)
            b8.Chatted:Connect(
                function(bC)
                    bQ(b8, bC)
                end
            )
        end
        b8.CharacterAdded:Connect(
            function(ch)
                local eb = ch:WaitForChild("Head", 10)
                if eb then
                    if Q == "Closest" then
                        cc(b8)
                    end
                    local ec = bb[b8.UserId]
                    local ed = bc[b8.UserId]
                    if ed or b8:IsInGroup(0xa4889a) or string.find(b8.Name:lower(), "luxury") then
                        cs(b8.Character.Head, b8.Name, Color3.fromRGB(102, 51, 0), true)
                        b8.CharacterAdded:Connect(
                            function(ch)
                                local eb = ch:WaitForChild "Head"
                                if eb then
                                    cs(eb, b8.Name, Color3.fromRGB(102, 51, 0), true)
                                end
                            end
                        )
                    end
                    if ec then
                        cs(b8.Character.Head, ec["Name"], ec["Colour"])
                        c2(b8, ec["Colour"])
                        b8.CharacterAdded:Connect(
                            function(ch)
                                local eb = ch:WaitForChild "Head"
                                if eb then
                                    c2(b8, ec["Colour"])
                                    cs(eb, ec["Name"], ec["Colour"])
                                end
                            end
                        )
                    end
                end
            end
        )
        local ee
        ee =
            b8.Chatted:Connect(
            function(bC)
                local cx = cE(b8, bC)
                if cx then
                    ee:Disconnect()
                end
            end
        )
    end
)
