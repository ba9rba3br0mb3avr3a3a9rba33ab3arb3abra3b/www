local function de(df, dg)
    if x or H then
        if dg == Enum.HumanoidStateType.FallingDown or dg == Enum.HumanoidStateType.PlatformStanding then
            LP.Character.Humanoid.PlatformStand = false
            LP.Character.Humanoid:ChangeState(8)
        end
    end
end
local function dh(bu)
    if bu then
        local c0 = bu:FindFirstChildOfClass "Tool"
        if c0 then
